{
    "name": "activecollab/jobsqueue",
    "type": "library",
    "description": "Simple jobs queue",
    "license": "MIT",
    "prefer-stable": true,
    "keywords": [
        "jobs",
        "queue",
        "activecollab"
    ],
    "homepage": "https://labs.activecollab.com",
    "authors": [
        {
            "name": "Ilija Studen",
            "email": "ilija.studen@activecollab.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "activecollab/containeraccess": "~1.0.0",
        "activecollab/databaseconnection": "dev-next",
        "doctrine/inflector": "^1.1.0",
        "psr/log": "~1.0"
    },
    "suggest": {
        "symfony/console": "Use Symfony console to easily create CLI jobs consumer"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.0",
        "monolog/monolog": "~1.0",
        "phpunit/phpunit": "~5.0",
        "pimple/pimple": "~3.0",
        "symfony/console": "~2.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "ActiveCollab\\JobsQueue\\": "src",
            "ActiveCollab\\JobsQueue\\Test\\": "test/src"
        }
    }
}
