{
    "name": "activecollab/jobsqueue",
    "type": "library",
    "description": "Simple jobs queue",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "jobs",
        "queue",
        "activecollab"
    ],
    "homepage": "https://labs.activecollab.com",
    "authors": [
        {
            "name": "Ilija Studen",
            "email": "ilija.studen@activecollab.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "activecollab/databaseconnection": "^5.0",
        "doctrine/inflector": "^1.1.0",
        "psr/log": "^1.0"
    },
    "suggest": {
        "ext-mysqli": "*",
        "ext-posix": "*",
        "symfony/console": "Use Symfony console to easily create CLI jobs consumer"
    },
    "require-dev": {
        "ext-mysqli": "*",
        "ext-posix": "*",
        "friendsofphp/php-cs-fixer": "^2.0",
        "monolog/monolog": "~1.0",
        "phpunit/phpunit": "^9.0",
        "pimple/pimple": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ActiveCollab\\JobsQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActiveCollab\\JobsQueue\\Test\\": "test/src"
        }
    }
}
